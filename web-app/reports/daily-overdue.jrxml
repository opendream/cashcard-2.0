<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="daily-overdue" language="groovy" pageWidth="595" pageHeight="842" columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="selectedDate" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     sum(transaction."amount") AS transaction_amount,
     period."due_date" AS period_due_date,
     member."lastname" AS member_lastname,
     contract."member_id" AS contract_member_id,
     member."firstname" AS member_firstname,
     contract."code" AS contract_code,
     period."amount" AS period_amount,
     period."no" AS period_no,
     loan_type."name" AS loan_type_name
FROM
     "public"."member" member INNER JOIN "public"."contract" contract ON member."id" = contract."member_id"
     INNER JOIN "public"."period" period ON contract."id" = period."contract_id"
     INNER JOIN "public"."loan_type" loan_type ON contract."loan_type_id" = loan_type."id"
     LEFT JOIN transaction ON period."id" = transaction."period_id"
WHERE
     period."due_date" < to_date($P{selectedDate}, 'yyyy-MM-dd')
     OR transaction."class" = 'th.co.opendream.cashcard.ReceiveTransaction'
     AND period."payoff_status" = false
GROUP BY
     loan_type."name",
     member."id",
     period."due_date",
     period."id",
     period."no",
     contract."id"
ORDER BY
     loan_type."name" ASC,
     member."firstname" ASC,
     member."lastname" ASC,
     period."due_date" DESC]]>
	</queryString>
