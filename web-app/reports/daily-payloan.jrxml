	<queryString>
		<![CDATA[SELECT
     contract."approval_date" AS contract_approval_date,
     contract."code" AS contract_code,
     contract."interest_rate" AS contract_interest_rate,
     contract."loan_amount" AS contract_loan_amount,
     contract."number_of_period" AS contract_number_of_period,
     loan_type."name" AS loan_type_name,
     member."firstname" AS member_firstname,
     member."lastname" AS member_lastname
FROM
     "public"."loan_type" loan_type INNER JOIN "public"."contract" contract ON loan_type."id" = contract."loan_type_id"
     INNER JOIN "public"."member" member ON contract."member_id" = member."id"
WHERE
     contract."payloan_date" between to_date($P{fromDate}, 'yyyy-MM-dd')
     AND to_date($P{toDate}, 'yyyy-MM-dd')]]>
	</queryString>
