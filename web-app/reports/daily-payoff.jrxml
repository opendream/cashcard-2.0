	<queryString>
		<![CDATA[SELECT
     transaction."amount" AS transaction_amount,
     transaction."class" AS transaction_class,
     transaction."differential" AS transaction_differential,
     transaction."fee" AS transaction_fee,
     transaction."fine" AS transaction_fine,
     contract."code" AS contract_code,
     contract."interest_rate" AS contract_interest_rate,
     contract."loan_amount" AS contract_loan_amount,
     contract."loan_balance" AS contract_loan_balance,
     member."firstname" AS member_firstname,
     member."lastname" AS member_lastname,
     period."payoff_date" AS period_payoff_date,
     period."payoff_status" AS period_payoff_status,
     period."status" AS period_status,
     transaction."date_created" AS transaction_date_created,
     period."no" AS period_no,
     loan_type."name" AS loan_type_name
FROM
     "public"."member" member INNER JOIN "public"."contract" contract ON member."id" = contract."member_id"
     INNER JOIN "public"."period" period ON contract."id" = period."contract_id"
     INNER JOIN "public"."loan_type" loan_type ON contract."loan_type_id" = loan_type."id"
     INNER JOIN "public"."transaction" transaction ON period."id" = transaction."period_id"

WHERE
     transaction."class" like 'th.co.opendream.cashcard.ReceiveTransaction'
AND  transaction."payment_date" between to_date($P{fromDate}, 'yyyy-MM-dd') AND to_date($P{toDate}, 'yyyy-MM-dd')]]>
	</queryString>
